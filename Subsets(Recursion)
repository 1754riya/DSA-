class Solution {
public:
 vector<vector<int>>result;
   void subsets(vector<int>&nums, int i,vector<int>temp){
    if(i>=nums.size()){
        result.push_back(temp);
        return;
    }
    temp.push_back(nums[i]);
    subsets(nums,i+1,temp); 
    temp.pop_back();
    subsets(nums,i+1,temp);

   }
    vector<vector<int>> subsets(vector<int>& nums) {
     vector<int>temp;
     subsets(nums,0,temp);  
     return result; 
    }
};
