class Solution {
  public:
    void rearrangeEvenOdd(Node *head) {
        Node*odd=head;
        Node*even=head->next;
        Node*evenHead=even;
        if(head==NULL || head->next==NULL) return ;
        while(even!=NULL && even->next!=NULL){
            odd->next=even->next;
            odd=odd->next;
            even->next=odd->next;
            even=even->next;
        }
        odd->next=evenHead;
        
    }
};
