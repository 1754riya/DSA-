class Solution {
public:
   vector<int>result;
   void solve(int curr,int n){
    if(curr>n) return;
    if(curr!=0){
        result.push_back(curr);
    }
    for(int append=0;append<=9;append++){
        int newnum=curr*10+append;
        if(newnum>n) return;
        if(newnum!=0) solve(newnum,n);
    }
   }
    vector<int> lexicalOrder(int n) {
        vector<int>temp;
        for(int i=1;i<=9;i++){
            solve(i,n);
        }
        return result;

    }
};
