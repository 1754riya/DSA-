
class Solution {
  public:
    vector<int> preOrder(Node* root) {
        stack<Node*>st;
        vector<int>result;
        if(root==NULL) return result;
        st.push(root);
        while(!st.empty()){
           Node*curr=st.top();
           st.pop();
           result.push_back(curr->data);
        if(curr->right!=NULL){
            st.push(curr->right);
        }
        if(curr->left!=NULL){
            st.push(curr->left);
        }
    }
    return result;
    }
};
