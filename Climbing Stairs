class Solution {
public:
   vector<int>dp;
    int climb(int n){
        if(n<=1) return 1;
        if(dp[n]!=-1) return dp[n];
        int left=climb(n-1);
        int right=climb(n-2);
        return dp[n]=left+right;
    }
    int climbStairs(int n) {
        dp.assign(n+1,-1);
        return climb(n);
    }
};
