class Solution {
public: 
     const int MOD = 1e9 + 7;
    long long power(long long a,long long b){
        if(b==0) return 1;
        long long half=power(a,b/2) %MOD;
        long long result=(half*half)%MOD ;
        if(b%2==1){
            result=(result*a)%MOD;
        }
        return result;
    }
    int countGoodNumbers(long long n) {
        long long odd=n/2;
        long long even =(n+1)/2;
        long ans= (power(5,even)*power(4,odd))%MOD;
        return (int) ans;

    }
};
